{
  "name": "microfrontend-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "start-app": "concurrently \"ionic serve --project=shell --port=8100\" \"ng serve mfe1 --port=8101\" \"ng serve mfe2 --port=8102\"",
